<%- include _head %>
<%- include _navigation %>
<div class="block-body column three-fourths">
  <% if(typeof list !='undefined'){ %>
    <% for(var i=0;i<list.length;i++){ %>
      <article>
        <header>
          <h2>
            <a href="/"><%= list[i].title %></a>
          </h2>
        </header>
        <div class="article-meta clearfix">
          <time class="left"><%= list[i].timeStr %></time>
          <ul class="tags left"></ul>
          <ul class="tags right">
            <% for(var j=0;j<list[i].tagsArray.length;j++){ %>
              <li>
                <a href="/"><%= list[i].tagsArray[j] %></a>
              </li>
            <% } %>
          </ul>
        </div>
        <div class="markdown-body">
          <%= list[i].post %>
        </div>
      </article>
    <% } %>
  <% } %>

  <%- include _pagination %>
</div>
<%- include _sidebar %>